#Auto-generated config file for voice enst-dennys-hsmm

name = enst-dennys-hsmm
locale = fr
fr-voice.version = 4.3.0
requires.fr.version = 4.3.0
requires.fr.download = http://mary.dfki.de/download/mary-install-4.x.x.jar

####################################################################
####################### Module settings  ###########################
####################################################################
# For keys ending in ".list", values will be appended across config files,
# so that .list keys can occur in several config files.
# For all other keys, values will be copied to the global config, so
# keys should be unique across config files.

hmm.voices.list = enst-dennys-hsmm

# If this setting is not present, a default value of 0 is assumed.
voice.enst-dennys-hsmm.wants.to.be.default = 0

# Set your voice specifications
voice.enst-dennys-hsmm.gender = male
voice.enst-dennys-hsmm.locale = fr
voice.enst-dennys-hsmm.domain = general
voice.enst-dennys-hsmm.samplingRate = 16000

# HMM Voice-specific parameters 
# parameters used during models training 
# MGC: stage=gamma=0 alpha=0.42 linear gain (default) 
# LSP: gamma>0  
#          LSP: gamma=1 alpha=0.0  linear gain/log gain 
#      Mel-LSP: gamma=1 alpha=0.42 log gain 
#      MGC-LSP: gamma=3 alpha=0.42 log gain 
voice.enst-dennys-hsmm.alpha = 0.42
voice.enst-dennys-hsmm.gamma = 0
voice.enst-dennys-hsmm.logGain = false

# Parameter beta for postfiltering 
voice.enst-dennys-hsmm.beta = 0.4

# HMM Voice-specific files
# Information about trees
voice.enst-dennys-hsmm.Ftd = jar:/marytts/voice/EnstDennysHsmm/tree-dur.inf
voice.enst-dennys-hsmm.Ftf = jar:/marytts/voice/EnstDennysHsmm/tree-lf0.inf
voice.enst-dennys-hsmm.Ftm = jar:/marytts/voice/EnstDennysHsmm/tree-mgc.inf
voice.enst-dennys-hsmm.Fts = jar:/marytts/voice/EnstDennysHsmm/tree-str.inf

# Information about means and variances PDFs 
voice.enst-dennys-hsmm.Fmd = jar:/marytts/voice/EnstDennysHsmm/dur.pdf
voice.enst-dennys-hsmm.Fmf = jar:/marytts/voice/EnstDennysHsmm/lf0.pdf
voice.enst-dennys-hsmm.Fmm = jar:/marytts/voice/EnstDennysHsmm/mgc.pdf
voice.enst-dennys-hsmm.Fms = jar:/marytts/voice/EnstDennysHsmm/str.pdf

# Information about Global Mean and Variance PDFs
voice.enst-dennys-hsmm.useGV = true
voice.enst-dennys-hsmm.maxMgcGvIter = 200
voice.enst-dennys-hsmm.maxLf0GvIter = 200
voice.enst-dennys-hsmm.Fgvf = jar:/marytts/voice/EnstDennysHsmm/gv-lf0-littend.pdf
voice.enst-dennys-hsmm.Fgvm = jar:/marytts/voice/EnstDennysHsmm/gv-mgc-littend.pdf
voice.enst-dennys-hsmm.Fgvs = jar:/marytts/voice/EnstDennysHsmm/gv-str-littend.pdf

# A context features file example for start-up testing.
voice.enst-dennys-hsmm.FeaFile = jar:/marytts/voice/EnstDennysHsmm/text_0324.pfeats

# Tricky phones file in case there were problematic phones during training, empty otherwise.
voice.enst-dennys-hsmm.trickyPhonesFile = jar:/marytts/voice/EnstDennysHsmm/trickyPhones.txt

# Information about Mixed Excitation
voice.enst-dennys-hsmm.useMixExc = true

# Filters for mixed excitation 
# File format: one column with all the taps, where the number of taps per filter = numTaps/numFilters 
voice.enst-dennys-hsmm.Fif = jar:/marytts/voice/EnstDennysHsmm/mix_excitation_filters.txt
# Number of filters in bandpass bank
voice.enst-dennys-hsmm.in = 5

# Information about acousticModels (if true allows prosody modification specified in MARYXML input)
voice.enst-dennys-hsmm.useAcousticModels = true

# acoustic models to use (HMM models or carts from other voices can be specified)
#(uncoment to allow prosody modification specified in MARYXML input)
voice.enst-dennys-hsmm.acousticModels = duration F0

voice.enst-dennys-hsmm.duration.model = hmm
voice.enst-dennys-hsmm.duration.attribute = d

voice.enst-dennys-hsmm.F0.model = hmm
voice.enst-dennys-hsmm.F0.attribute = f0
